{
  "name": "@automatons/tools",
  "version": "1.0.167",
  "repository": "git@github.com:openapi-automatons/tools.git",
  "author": "tanmen <yt.prog@gmail.com>",
  "license": "MIT",
  "keywords": [
    "openapi",
    "openapi-automatons",
    "automatons"
  ],
  "main": "index.js",
  "scripts": {
    "prebuild": "depcheck && node scripts/prebuild.js",
    "build": "tsc && cp package.json README.md LICENSE dist",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "postinstall": "husky install && typesync && yarn check --integrity || yarn install --ignore-scripts && yarn-deduplicate",
    "postversion": "cp package.json ../package.json"
  },
  "dependencies": {
    "fs-extra": "^10.1.0",
    "got": "^11.8.5",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "@types/eslint": "^8.4.6",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.5.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.8.3",
    "@types/semantic-release": "^17.2.4",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.25.0",
    "eslint-config-google": "^0.14.0",
    "husky": "^8.0.1",
    "jest": "^26.0.1",
    "lint-staged": "^13.0.3",
    "semantic-release": "^19.0.5",
    "ts-jest": "^26.0.0",
    "typescript": "^4.7.4",
    "typesync": "^0.9.2",
    "yarn-deduplicate": "^6.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
